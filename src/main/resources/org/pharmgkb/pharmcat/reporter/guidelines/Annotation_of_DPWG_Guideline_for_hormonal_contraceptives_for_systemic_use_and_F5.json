    },
    "implications" : {
      "id" : 1451694723,
      "html" : "<p>The guideline does not provide a description of the impact of the absence of Factor V Leiden on estrogen-containing contraceptives.</p>\n",
      "version" : 0
    },
    "phenotype" : {
      "id" : 1451694724,
      "html" : "<p>No Factor V Leiden variant.</p>\n",
      "version" : 0
    },
    "recommendation" : {
      "id" : 1451694725,
      "html" : "<p>The guideline does not provide a recommendation for estrogen-containing contraceptives in patients who do not have the Factor V Leiden variant.</p>\n",
      "version" : 0
    },
